      @! &#191;Ser Horuss?
                </args>
                <action command='removeAction'>
                  <args>horuss,Ser Horuss.,Dar Rocintauro a Horuss.</args>
                  <action command='removeAction'>
                    <args>horsaponi,Examinar.</args>
                    <action command='addAction'>
                      <args>horsaponi,
                        <action command='talk' name='Examinar.'>
                              <args>
                              @!:#iiiih Tu querido, dulce, bonito, querido, hermoso, dulce, dulce, querido y dulce ROCINTAURO. 
                              <!--  -->
                              </args>
                        </action>
                      </args>
